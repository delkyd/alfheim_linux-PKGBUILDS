Connection='wireless'
Interface=wlp3s0
Security='wpa-configsection'
Description="eduroam network"
IP='dhcp'
TimeoutWPA=30
WPAConfigSection=(
    'ssid="eduroam"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'proto=WPA2'
    'phase2="auth=MSCHAPV2"'
    'anonymous_identity="xxxxx@kth.se"'
    'identity="xxxxx@kth.se"'
    'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
    'password="xxxxxxxx"'
)