post-install() {
# icon cache

gtk-update-icon-cache Yltra-Flat-Arch-Blue
gtk-update-icon-cache Yltra-Flat-Majestic
gtk-update-icon-cache Yltra-Flat-Mint-Green
gtk-update-icon-cache Yltra-Flat-Orange
gtk-update-icon-cache Yltra-Flat
 
}

post-upgrade() {
  post-install
}

post-remove() {
  post-install
}

