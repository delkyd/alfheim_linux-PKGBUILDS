pkgname=meliaesvg-dark-icon-theme
pkgver=1.2
pkgrel=1
pkgdesc='MeliaeSVG icon theme set - Dark/Light Panels version'
arch=('any')
url='http://sora-meliae.deviantart.com/'
license=('GPL')
source=('http://orig14.deviantart.net/4b72/f/2010/065/1/7/meliae_svg_dark_v__1_2_by_sora_meliae.zip')
md5sums=('5deea7afa77d896a384bbc7dfee52d32')

package() {
    mkdir -p $pkgdir/usr/share/icons/
    cp -r MeliaeSVG-Dark $pkgdir/usr/share/icons/
}

