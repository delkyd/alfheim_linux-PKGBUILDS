pkgname=xclm-dirs
pkgver=1.00
pkgrel=3
pkgdesc="xclm and etc directories for Microchip software"
arch=(any)
url='https://aur.archlinux.org/packages.php?ID=57960'
license=(custom)
options=(emptydirs)

package() {
  mkdir -p $pkgdir/opt/microchip/xclm/license
  mkdir -p $pkgdir/opt/microchip/xc8/v1.34/etc
}
